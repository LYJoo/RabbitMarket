<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
	"-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd"
 >
 
 <!-- 누가 이 XML을 사용할까?(mybatis) / 누구와 연결을? (해당 DAO) -->
 <mapper namespace="com.market.rabbit.member.dao.ProfileDAO1">
 
	<select id="wishlist" resultType="wish">
 		SELECT * FROM WISH ORDER BY wish_idx DESC
 	</select>
 	
 	<delete id="delete">
 		DELETE FROM WISH WHERE wish_idx=#{param1}
 	</delete>
 	
 </mapper>